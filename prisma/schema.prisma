generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// Todo 테이블
model Todo {
  idx        Int       @id @default(autoincrement())
  memberId   String?   @map("member_id") @db.VarChar(45)
  projectId  String?   @map("project_id") @db.VarChar(45)
  subject    String?   @db.VarChar(45)
  content    String?   @db.VarChar(45)
  finish     String?   @db.VarChar(45)
  date       DateTime? @db.Date

  @@map("todo")
}